    <form action = "/customer/newOrder" method = "POST">
      <label for = "w_id">Select Warehouse:</label>
      <input type = "number" id = "w_id" name="w_id" required>
      <select id="cars" name="w_id">
        <option value = "choose">Choose Warehouse ID</option>
        <% if(warehouseData.length > 0) { %>
              <% warehouseData.forEach(eachWarehouse => {
              let warehouseId = eachWarehouse['w_id'] %>
              <option value="<%= warehouseId %>"><%= warehouseId %></option>
          <% })}%>
      </select>
      <label for = "d_id">Select District: </label>
      <input type = "number" id = "d_id" name = "d_id" required>
      <select id="cars" name="d_id">
        <option value = "choose">Choose District ID</option>
        <% if(districtData.length > 0) { %>
          <% districtData.forEach(eachDistrict => {
          let districtId = eachDistrict['d_id'] %>
          <option value="<%= districtId %>"><%= districtId %></option>
      <% })}%>
        <!-- <option value=""></option> -->
      </select>
      <label for = "c_id">Select Customer: </label>
      <input type = "number" id = "c_id" nmae = "c_id" required>
      <select id="cars" name="c_id">
        <option value = "choose">Choose Customer ID</option>
        <% if(customerData.length > 0) { %>
          <% customerData.forEach(eachCustomer => {
          let customerId = eachCustomer['c_id'] %>
          <option value="<%= customerId %>"><%= customerId %></option>
      <% })}%>
      </select>
